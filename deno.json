{
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": ["deno.window"],
    "strict": true
  },
  "tasks": {
    "dev": "deno run --allow-all --watch index.ts",
    "start": "deno run --allow-all index.ts",
    "deploy-commands": "deno run --allow-all deploy-commands.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "deno run --allow-all database/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "imports": {
    "@std/dotenv": "https://deno.land/std@0.224.0/dotenv/mod.ts",
    "@std/log": "https://deno.land/std@0.224.0/log/mod.ts",
    "canvas": "npm:canvas@^3.0.0",
    "@discordjs/builders": "npm:@discordjs/builders@^1.13.0",
    "@discordjs/rest": "npm:@discordjs/rest@^2.0.0",
    "discord-api-types": "npm:discord-api-types@^0.37.0",
    "discord.js": "npm:discord.js@^14.24.2",
    "drizzle-orm": "npm:drizzle-orm@^0.38.3",
    "drizzle-kit": "npm:drizzle-kit@^0.30.1",
    "html-entities": "npm:html-entities@^2.3.2",
    "moment": "npm:moment@^2.29.1",
    "moment-timezone": "npm:moment-timezone@^0.5.34",
    "mysql2": "npm:mysql2@^3.10.1",
    "node-cron": "npm:node-cron@^3.0.0",
    "turndown": "npm:turndown@^7.1.1"
  }
}
